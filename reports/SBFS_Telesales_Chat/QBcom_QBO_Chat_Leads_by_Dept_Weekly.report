<?xml version="1.0" encoding="UTF-8"?>
<Report xmlns="http://soap.sforce.com/2006/04/metadata">
    <aggregates>
        <calculatedFormula>Lead.Time_to_Conversion__c:SUM</calculatedFormula>
        <datatype>number</datatype>
        <description>Average Lead Handle Time</description>
        <developerName>FORMULA1</developerName>
        <isActive>false</isActive>
        <isCrossBlock>false</isCrossBlock>
        <masterLabel>AHT</masterLabel>
        <scale>2</scale>
    </aggregates>
    <columns>
        <field>User$Name</field>
    </columns>
    <currency>USD</currency>
    <description>QBcom QBO Chat Leads - Weekly - By Dept</description>
    <filter>
        <criteriaItems>
            <column>User$Division</column>
            <isUnlocked>false</isUnlocked>
            <operator>contains</operator>
            <value>Chat</value>
        </criteriaItems>
        <criteriaItems>
            <column>User.Leads__r$Country</column>
            <isUnlocked>false</isUnlocked>
            <operator>notEqual</operator>
            <value>Canada,United Kingdom</value>
        </criteriaItems>
        <language>en_US</language>
    </filter>
    <format>Matrix</format>
    <groupingsAcross>
        <dateGranularity>Day</dateGranularity>
        <field>User.Leads__r$CreatedDate</field>
        <sortOrder>Asc</sortOrder>
    </groupingsAcross>
    <groupingsDown>
        <dateGranularity>Day</dateGranularity>
        <field>User$Department</field>
        <sortOrder>Asc</sortOrder>
    </groupingsDown>
    <name>QBcom QBO Chat Leads by Dept - Weekly</name>
    <params>
        <name>co</name>
        <value>1</value>
    </params>
    <reportType>User_Data_and_Leads__c</reportType>
    <scope>organization</scope>
    <showDetails>false</showDetails>
    <timeFrameFilter>
        <dateColumn>User.Leads__r$CreatedDate</dateColumn>
        <interval>INTERVAL_THISWEEK</interval>
    </timeFrameFilter>
</Report>
