<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <webLinks>
        <fullName>Upload_Signature_Image</fullName>
        <availability>online</availability>
        <displayType>link</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Upload Image Signature</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>var userId = &apos;{!User.Id}&apos;;
		var popupWindow = null;
		var width =  800;
		var height=  300;
		if(userId!=null){
		var leftPosition, topPosition;
		leftPosition = (window.screen.width / 2) - ((width / 2) + 10);
		topPosition = (window.screen.height / 2) - ((height/ 2) + 50);
		var strFeatures = &apos;menubar=no,location=no,resizable=no,scrollbars=no,status=no,channelmode=no,directories=no,width=&apos;+width+&apos;px,height=&apos;+height+&apos;px,titlebar=no,toolbar=no,modal=yes,left=&apos;+leftPosition+&apos;,top=&apos;+topPosition;
		var url=&apos;/apex/SignatureImageUpload?id=&apos;+userId;
		popupWindow = window.open(url,&quot;UploadImageSignature&quot;, strFeatures); 
		if(popupWindow != null){
			popupWindow.focus();
		} 
    
		}
		else{
			alert(&apos;Record not found.&apos;);
		}

		window.onbeforeunload = function() {
			if(popupWindow != null){
				popupWindow.close();
			}
		};</url>
    </webLinks>
</CustomObject>
