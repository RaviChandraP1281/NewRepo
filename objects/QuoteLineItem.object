<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <validationRules>
        <fullName>Quote_Lines_Locked_when_Quote_is_Locked</fullName>
        <active>true</active>
        <description>Prevents a user from changing the Quote Line Items when the Quote they&apos;re associated to is Locked unless they are a System Adminsitrator</description>
        <errorConditionFormula>Quote_IsLocked__c = &quot;TRUE&quot; &amp;&amp;
 $User.ProfileId &lt;&gt; &quot;00ei0000000ozEV&quot;</errorConditionFormula>
        <errorMessage>The associated quote has been accepted, therefore the associated Quote Line Items can not be modified</errorMessage>
    </validationRules>
</CustomObject>
