<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <validationRules>
        <fullName>Quote_locked_after_Accepted</fullName>
        <active>true</active>
        <description>Prevents the quote from being edited once it becomes Accepted, except for System Administrators</description>
        <errorConditionFormula>IF( IsLocked__c = TRUE, TRUE, FALSE) &amp;&amp;
 $User.ProfileId &lt;&gt; &quot;00ei0000000ozEV&quot;</errorConditionFormula>
        <errorMessage>Once the Quote Status = Accepted, the quote is Locked</errorMessage>
    </validationRules>
</CustomObject>
