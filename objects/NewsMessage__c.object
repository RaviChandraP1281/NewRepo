<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <validationRules>
        <fullName>ProductORSystemImpactedRequired</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISBLANK(System_Impacted__c ), ISBLANK(Product__c))</errorConditionFormula>
        <errorMessage>Product or System Impacted is required</errorMessage>
    </validationRules>
</CustomObject>
