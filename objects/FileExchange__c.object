<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <validationRules>
        <fullName>BlockBetaTestUsersFromEnteringCompany</fullName>
        <active>true</active>
        <description>BlockBetaTestRecordsFromHavingCompany</description>
        <errorConditionFormula>TEXT(Type__c) == &apos;BetaTesting&apos; &amp;&amp;  NOT(ISBLANK(Company__c))</errorConditionFormula>
        <errorDisplayField>Company__c</errorDisplayField>
        <errorMessage>Company is not allowed for Beta Test Folders. Please remove the value and save the record .</errorMessage>
    </validationRules>
</CustomObject>
