<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <enableEnhancedLookup>true</enableEnhancedLookup>
    <validationRules>
        <fullName>Account_Contact_Required</fullName>
        <active>false</active>
        <description>Validation rule specifies that Account and Contact cannot be left empty.</description>
        <errorConditionFormula>ISBLANK(Account__c)  ||  ISBLANK(Contact__c)</errorConditionFormula>
        <errorMessage>Account and Contact are mandatory while creating Training &amp; Memberships. Please specify Account and/or Contact.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>End_Date_should_be_greater_than_Start_Da</fullName>
        <active>true</active>
        <errorConditionFormula>End_Date__c  &lt;  Start_Date__c</errorConditionFormula>
        <errorDisplayField>End_Date__c</errorDisplayField>
        <errorMessage>Training End Date should be greater than Start Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Expiry_Date_grt_than_St_Date</fullName>
        <active>true</active>
        <errorConditionFormula>Expiry_Date__c &lt; Start_Date__c</errorConditionFormula>
        <errorDisplayField>Expiry_Date__c</errorDisplayField>
        <errorMessage>Training Expiry Date should be greater than Start Date</errorMessage>
    </validationRules>
</CustomObject>
