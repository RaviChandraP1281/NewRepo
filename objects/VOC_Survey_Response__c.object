    <fields>
        <fullName>Avg_Score__c</fullName>
        <description>The custom field is added as part of SA-639. The field will calculate the average score for Sales VOC survey.</description>
        <externalId>false</externalId>
        <formula>IF((If(ISBLANK(Response_1_Numeric__c) ,0,1) + IF(ISBLANK(Response_3_Numeric__c),0,1)+if(ISBLANK(Response_4_Numeric__c),0,1))&gt;0,((IF(ISBLANK(Response_1_Numeric__c),0,Response_1_Numeric__c) + IF(ISBLANK( Response_3_Numeric__c ),0, Response_3_Numeric__c ) + if(ISBLANK( Response_4_Numeric__c ),0, Response_4_Numeric__c ))
/
(If(ISBLANK(Response_1_Numeric__c) ,0,1) + IF(ISBLANK(Response_3_Numeric__c),0,1)+if(ISBLANK(Response_4_Numeric__c),0,1))), 0)</formula>
        <label>Avg Score</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
