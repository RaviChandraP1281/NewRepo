<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <validationRules>
        <fullName>Value_Rank_Required_for_Auto_Renewals</fullName>
        <active>true</active>
        <description>Value Rank field value is required when customer Renewal preference was set to Auto Renewal.</description>
        <errorConditionFormula>ISPICKVAL( Renewal_Preference__c , &apos;Auto Renewal&apos;) &amp;&amp;  ISPICKVAL( Value_Rank__c, &apos;&apos; )</errorConditionFormula>
        <errorDisplayField>Value_Rank__c</errorDisplayField>
        <errorMessage>Value Rank is required when Renewal Preference is &apos;Auto Renewal&apos;.</errorMessage>
    </validationRules>
</CustomObject>
