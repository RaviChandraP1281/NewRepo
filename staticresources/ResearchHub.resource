id.model.getModel('SearchStudies');           \n            getconditiondata('Strategic_Priority_Aligned__c',0,study,search);\n\t\t\tgetconditiondata('Customer_Benefits__c',1,study,search);\n\t\t\tgetconditiondata('Customer_Segment__c',2,study,search);\n\t\t\tgetconditiondata('Product__c',3,study,search);\n            getconditiondata('Study_Type__c',4,study,search);\n\t   \tstudy.load(function(){skuid.$.unblockUI();});  \t\n\t}    \n}\n\nfunction getconditiondata(ConditionField,index,study,search){\n        study.conditions[index].values = [];\n\t\tif(ConditionField ==='Strategic_Priority_Aligned__c'){\n             if(search.data[0][ConditionField] !== undefined &amp;&amp; search.data[0][ConditionField] !== ''){\n                study.conditions[index].value =''+ search.data[0][ConditionField];\n                study.conditions[index].values.push(study.conditions[index].value);\n                study.conditionLogic = \"(1 AND 2 AND 3 AND 4 AND 5 AND 6 AND 7)\";\n             } \n             else{\n                 delete study.conditions[index].value;\n                 delete study.conditions[index].values;\n             } \n        }         \n        if(ConditionField ==='Customer_Benefits__c'){\n             if(search.data[0][ConditionField] !== undefined &amp;&amp; search.data[0][ConditionField] !== ''){\n                study.conditions[index].value =''+ search.data[0][ConditionField];\n                study.conditions[index].values.push(study.conditions[index].value);\n                study.conditionLogic = \"(1 AND 2 AND 3 AND 4 AND 5 AND 6 AND 7)\";\n             } \n             else{\n                 delete study.conditions[index].value;\n                 delete study.conditions[index].values;\n             } \n        } \n        if(ConditionField ==='Customer_Segment__c'){\n             if(search.data[0][ConditionField] !== undefined &amp;&amp; search.data[0][ConditionField] !== ''){\n                study.conditions[index].value =''+ search.data[0][ConditionField];\n                study.conditions[index].values.push(study.conditions[index].value);\n                study.conditionLogic = \"(1 AND 2 AND 3 AND 4 AND 5 AND 6 AND 7)\";\n             } \n             else{\n                 delete study.conditions[index].value;\n                 delete study.conditions[index].values;\n             } \n        } \n        if(ConditionField ==='Product__c'){\n             if(search.data[0][ConditionField] !== undefined &amp;&amp; search.data[0][ConditionField] !== ''){\n                study.conditions[index].value =''+ search.data[0][ConditionField];\n                study.conditions[index].values.push(study.conditions[index].value);\n                study.conditionLogic = \"(1 AND 2 AND 3 AND 4 AND 5 AND 6 AND 7)\";\n             } \n             else{\n                 delete study.conditions[index].value;\n                 delete study.conditions[index].values;\n             } \n        } \n        if(ConditionField ==='Study_Type__c'){\n             if(search.data[0][ConditionField] !== undefined &amp;&amp; search.data[0][ConditionField] !== ''){\n                study.conditions[index].value =''+ search.data[0][ConditionField];\n                study.conditions[index].values.push(study.conditions[index].value);\n                study.conditionLogic = \"(1 AND 2 AND 3 AND 4 AND 5 AND 6 AND 7)\";\n             } \n             else{\n                 delete study.conditions[index].value;\n                 delete study.conditions[index].values;\n             } \n        } \t        \n    return study;\n}\n\n/*SRS-440*/\nfunction openEmailPopwindow(rowId){\n    var getStudyModel = skuid.$M('GetStudy');\n    var rowCondtion = getStudyModel.getConditionByName('Id',false);\n    getStudyModel.setCondition(rowCondtion,rowId,true);\n    //getStudyModel.updateData();\n    getStudyModel.load(function(varData){\n        console.log(varData.models.GetStudy.data.length);\n           if(varData.models.GetStudy.data.length &gt; 0){\n               if(varData.models.GetStudy.data[0].Email__c !== undefined){\n                    var newLine = escape('\\n');\n                    var email = varData.models.GetStudy.data[0].Email__c;\n                    var subject = 'Request For Information on Your Research Study';\n                    var emailBody = 'Dear '+ varData.models.GetStudy.data[0].First_Name__c +', '+newLine+newLine\n                                    +'I am reaching out to you regarding'+ varData.models.GetStudy.data[0].Name +'study on '+varData.models.GetStudy.data[0].Start_Date_of_Study__c+' to '+varData.models.GetStudy.data[0].End_Date_of_Study__c +'. I would like to understand more about your learnings and insights. Below is more information about your study.'+newLine+newLine\n                                    +'Customer Segment : '+varData.models.GetStudy.data[0].Customer_Segment__c+newLine\n                                    +'Product(s) : '+varData.models.GetStudy.data[0].Product__c+newLine\n                                    +'Strategic Priority(s) : '+varData.models.GetStudy.data[0].Strategic_Priority_Aligned__c+newLine\n                                    +'Customer Benefit(s) : '+varData.models.GetStudy.data[0].Customer_Benefits__c+newLine\n                                    +'Study Type : '+varData.models.GetStudy.data[0].Study_Type__c+newLine+newLine\n                                    +'I will set up time with you discuss in more detail.'+newLine+newLine\n                                    +'Thank you.'+newLine;    \n                    window.location.href = 'mailto:' + email + '?subject=' + subject + '&amp;body=' + emailBody;\n               }\n               else{\n                   alert('No Email address found !');\n               }\n            }\n    });\n}</jsitem><jsitem location=\"inlinesnippet\" name=\"Search\" cachelocation=\"false\">searchValues();</jsitem></javascript></resources></skuidpage>","skuid__Layout2__c":null,"skuid__Layout3__c":null,