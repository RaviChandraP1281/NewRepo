ding=\"bottom,right,left,\"><styles><styleitem property=\"border-left\" value=\"10px solid transparent\"/><styleitem property=\"border-right\" value=\"10px solid transparent\"/><styleitem property=\"border-bottom\" value=\"10px solid transparent\"/><styleitem property=\"padding-left\" value=\"10px\"/><styleitem property=\"padding-right\" value=\"10px\"/><styleitem property=\"padding-bottom\" value=\"10px\"/><styleitem property=\"box-sizing\" value=\"border-box\"/></styles></styleitem><styleitem type=\"size\"/></styles></wrapper></components></division></divisions><styles><styleitem type=\"background\" bgtype=\"none\"/></styles></grid></components><resources><labels/><javascript><jsitem location=\"inline\" name=\"PageInit\" cachelocation=\"false\" url=\"\">var service_response = false;\n(function(skuid){\n\tvar $ = skuid.$;\n\t$(document.body).one('pageload',function(){\n\t    var $ = skuid.$;\n\t    setKBDocumentLink();\n        skuid.$.blockUI();\n        if(skuid.page.params['from'] === 'acct'){\n    \t\tgetProductsAndPaymentInfo(true,skuid.$M('AssetModel').getFirstRow().Status !== 'In Trial',false);\n            moveToWizardPage('products');\n            updateUserSelection('products');\n        }else if(skuid.page.params['from'] === 'lead'){\n            getLeadRelatedAccountInformation();\n        }else{\n            skuid.$.unblockUI();\n        }\n\t});\n})(skuid);\n\nfunction backToSuggestedAccount(){\n    var searchSelectionModel = skuid.model.getModel('SearchSelection'),\n    searchSelectionRow = searchSelectionModel.getFirstRow();\n    searchSelectionModel.updateRow(searchSelectionRow, 'IsStanderedSearch__ui', true);\n    searchSelectionModel.updateRow(searchSelectionRow, 'IsSearchedUsingCustom__ui', false);\n    getLeadRelatedAccountInformation();\n}\n\nfunction getProductsAndPaymentInfo(isProductService,isPaymentService,isProductSubmissionService){\n    skuid.$.blockUI();\n    var services = [];\n    if(isProductService){\n        services.push(createProductService())\n    }\n    if(isPaymentService){\n        services.push(paymentInfoService())\n    }\n    if(isProductSubmissionService){\n        services.push(createProductSubmissionService());\n    }\n    ContinuationService.executeServices(services,'OnlineSalesIncludeAddOnServiceHelper');\n}\n\nfunction highlightRow(id) {\n\t    \n    // ======================================\n    // Set up models and table component\n    // ======================================\n    var tableName = [id];\n\n    // ======================================\n    // Highlight the row based on user click\n    // ======================================\n    // For each table row, add CSS class 'highlighted-row' on click and fill in circle\n    // Ensure only one row is selected at all times.  Check all other rows and remove selected row classes as needed\n    var selectedRow = '';\n    for (i = 0; i&lt;tableName.length; i++) {\n\t\tskuid.$(tableName[i] + ' table tbody tr').click(function(){\n\t\t    selectedRow = this;\n\t\t    skuid.$(selectedRow).addClass('highlighted-row');\n\t\t    iconElement = skuid.$(selectedRow).find('.nx-skootable-buttonicon');\n\t\t    skuid.$(iconElement).removeClass('fa-circle-o').addClass('fa-dot-circle-o');\n\t\t    for (j = 0; j&lt;tableName.length; j++) {\n    \t\t    // Ensure only one row is selected\n    \t\t    skuid.$(tableName[j] + ' table tbody tr').each(function(){\n    \t\t        if ((this != selectedRow) &amp;&amp; (skuid.$(this).hasClass('highlighted-row'))) {\n    \t\t            skuid.$(this).removeClass('highlighted-row');\n    \t\t            iconElement = skuid.$(this).find('.nx-skootable-buttonicon');\n    \t\t            skuid.$(iconElement).removeClass('fa-dot-circle-o').addClass('fa-circle-o');\n    \t\t        }\n    \t\t    });\n    \t\tif(skuid.$(this).closest('table').closest('#UpgradeTable').attr('id') === 'UpgradeTable' )\n\t\t        updateUpgradeAssetInfo();\n\t\t    }\n\t\t});\n    }\n}\n\nfunction addErrorToAccountInfoTab