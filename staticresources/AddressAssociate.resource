Model('AccountSearch');\n      \n         var workingRow = myContactModel.getFirstRow();\n         myContactModel.updateRow(workingRow, { Address__c : addressID });\n         \n         myContactModel.save({callback: function(result){\n           \n\t\t\t\tif (result.totalsuccess)\n    \t\t\t\t{\n    \t\t\t\t     window.open('/'+myContactModel.data[0].Id +'','_self');\n    \t\t\t\t}\n            \n                else \n                    {\n                        console.log(result.insertResults);\n                        console.log(result.updateResults);\n                        console.log(result.deleteResults);\n                    }\n             \n         }});\n         \n        \n   \n}</jsitem><jsitem location=\"inline\" name=\"pageInit\" cachelocation=\"false\" url=\"\">(function(skuid){\n\tvar $ = skuid.$;\n    $(document).ready(function(){ \n           \n            \tvar myAccountModel = skuid.model.getModel('AccountSearch');\n           \n                myAccountModel.updateData(function(){\n                    skuid.model.updateData(skuid.model.list());\n                }); \n        \n    \t\t\n     });\n})(skuid);</jsitem></javascript><css><cssitem location=\"inline\" name=\"searchComponent\" cachelocation=\"false\">.globalSearch .ui-autocomplete-input\n{\n   width:50%;\n   background:white;\n   border:1px solid #cacaca;\n   \n}\n\n/*\n    filterExport.css: Places the cloned filterExport button on the right side of the Table, replaces export icon, \n                        and hides the original button.\n    \n    Instructions:\n        1) filterExport_inline and filterExport_css must be present on the Skuid page's inline JS and CSS, respectively.\n        2) Make sure icon_excel_export exists in the Static resources, and the content URL in the CSS is correct.\n        3) For every Table you want to apply this to:\n            a) check \"Show Export Button\" attribute.\n            b) Table -&gt; Display -&gt; Advanced\n                i) add \"filterExportTable\" to the CSS class(es).\n                ii) display Table Buttons on the left (otherwise the excel button will show up far left by design).\n    Notes:\n        - filterExport.css will display:none the original export button, but since the Intuit \n            Theme hides the footer by default this won't be apparent.\n        - Use Case is to have a custom Export button with a custom icon on the rightmost of a Table's header.\n*/\n\n.filterExportTable .nx-editor-header .nx-export-btn{\n    cursor: pointer;\n    vertical-align: middle;\n    margin-top: 0px;\n    position: relative;\n    margin-left: 8px;\n    padding: 4px 8px 3px 5px;\n    border: 1px solid #babec5;\n    border-radius: 2px;\n    /* Add Gradient to button (http://cssgradientbutton.com/)*/\n    background-color: #f4f5f5; background-image: -webkit-gradient(linear, left top, left bottom, from(#f4f5f5), to(#dfdddd));\n    background-image: -webkit-linear-gradient(top, #f4f5f5, #dfdddd);\n    background-image: -moz-linear-gradient(top, #f4f5f5, #dfdddd);\n    background-image: -ms-linear-gradient(top, #f4f5f5, #dfdddd);\n    background-image: -o-linear-gradient(top, #f4f5f5, #dfdddd);\n    background-image: linear-gradient(to bottom, #f4f5f5, #dfdddd);filter:progid:DXImageTransform.Microsoft.gradient(GradientType=0,startColorstr=#f4f5f5, endColorstr=#dfdddd);\n}\n\n.filterExportTable .nx-editor-header .nx-export-btn:hover{\n    background-color: #d9dddd; background-image: -webkit-gradient(linear, left top, left bottom, from(#d9dddd), to(#c6c3c3));\n    background-image: -webkit-linear-gradient(top, #d9dddd, #c6c3c3);\n    background-image: -moz-linear-gradient(top, #d9dddd, #c6c3c3);\n    background-image: -ms-linear-gradient(top, #d9dddd, #c6c3c3);\n    background-image: -o-linear-gradient(top, #d9dddd, #c6c3c3);\n    background-image: linear-gradient(to bottom, #d9dddd, #c6c3c3);filter:progid:DXImageTransform.Microsoft.gradient(GradientType=0,startColorstr=#d9dddd, endColorstr=#c6c3c3);\n}\n\n/*.filterExport-floatright{\n    float: right;\n}\n.filterExport-floatleft{\n    float: left;\n}*/\n\n.filterExportTable .nx-list-footer .nx-export-btn{\n    display: none; /* Hides the original button */\n}\n/* \n    This replaces the icon of the original excel export. Make sure this icon exists in the Static Resources!\n*/\n\n.filterExportTable .sk-icon-export:before, .nx-picker-body .nx-picker-icon-wrapper .sk-icon-export:before{\n    content: url(/resource/customIcons/excel.png); /* CONTENT URL */\n}\n\n.nx-header-left, .nx-footer-left\n{\ndisplay:none;\n}\n/* Styling to add pixels to the top of each sectional wrapper for space */\n.spaceTop{\n    padding-top: 12px;\n}\n\n.nx-modified .nx-fieldtext {\n    font-style: normal !important;\n}\n\n.addAlert .nx-field .nx-fieldtext {\n    color: red !important;\n    font-weight: bold;\n}\n\n/* On header items to make the display consistent */\n.headerText{\n    font-size:1.3em;\n    color:gray;\n}\n\n/* The partners list tabled needed the export which is hidden on the primary CSS. This overrides the footer to show */\n.showFooter .nx-list-footer {\n    display: block;\n}\n\n/*For some reason, .searchIcon is being used elsewhere and weird override-y things are happening when we use it\n    So, for the sake of just having it work, I c/ped the original .searchIcon .searchbox selector under a new moniker.\n*/\n.searchIconTable .searchbox{\n    background-image: url(\"/resource/customIcons/search.png\");\n    background-repeat: no-repeat;\n    background-size: 15px;\n    background-position: right 10px top 50%;\n    padding-left: 10px !important; /*Since this will be overidden by Theme Editor, this property is also specified in Table-&gt;Searchbox-&gt;Padding*/\n}\n\n.quarterText {\n    float: right;\n    margin-top: 36px;\n    margin-bottom: -36px;\n    padding-right: 5px;\n}\n\n/* Table Formatting - Footer */\n.showFooter .nx-list-footer-left {\n    float: left;\n}\n.showFooter .nx-list-footer-center {\n    margin-top: 10px !important;\n    float: right;\n}\n.showFooter .nx-list-footer-right {\n    margin-top: 2px;\n    text-align: center !important;\n}\n.showFooter .nx-buttonset .ui-button {\n    border: 1px solid #babec5 !important;\n    padding: 5px 10px 5px 10px !important;\n    border-radius: 0px !important;\n    margin-right: -0.5px !important;\n    margin-left: -0.5px !important;\n    color: #0097E6 !important;\n}\n.showFooter .nx-buttonset .ui-button.nx-button-selected {\n    background-color: #0077C5 !important; \n}\n.showFooter .nx-buttonset .ui-button.nx-button-selected .ui-button-text {\n    color: white !important;\n}\n.showFooter .nx-buttonset .ui-button.ui-button-disabled {\n    color: #BABEC5 !important;\n}\n.showFooter .nx-buttonset .ui-button:hover {\n    background-color: #ECEEF1 !important;\n}\n\n.showFooter .nx-buttonset .first {\n    border-top-left-radius:2px !important;\n    border-bottom-left-radius:2px !important;\n}\n.showFooter .nx-buttonset .last {\n    border-top-right-radius:2px !important;\n    border-bottom-right-radius:2px !important;\n}\n\n.showFooter .nx-list-footer-left select {\n    background-image: url(\"/resource/customIcons/arrows/upDown.png\"), !important;\n    background-repeat: no-repeat;\n    background-position: 90% 50%;\n    /*-webkit-appearance:none;\n    -moz-appearance: none;*/\n    padding: 5px 15px 5px 6px;\n    font-size: 14px;\n    height: 30px;\n    margin-top: 0px;\n    margin-bottom: 20px;\n}\n\n/* Table column text alignments */\n#partnerTable tr&gt;*:first-child {\n    display: none;\n}\n#partnerTable tr&gt;td:nth-child(2) {\n    border-left: 1px dotted #8d9096 !important;\n}\n#partnerTable tr&gt;td:nth-child(2) .nx-field {\n    line-height: 0px;\n}\n#partnerTable th:nth-child(2), th:nth-child(3) {\n  text-align: center;\n}\n#partnerTable th:nth-child(11) {\n  text-align: center;\n}\n#partnerTable tr&gt;td:nth-child(2), tr&gt;td:nth-child(3), tr&gt;td:nth-child(12) {\n  text-align: center;\n}\n#partnerTable tr&gt;td:nth-child(8), td:nth-child(9), tr&gt;td:nth-child(10) {\n  text-align: right;\n}\n\n.taskTable td {\n    text-align: left !important;\n}\n.nx-massactions {\n    display: none !important;\n}\n.displayNone {\n    display: none;\n}\n\n/* Add x back into popups */\n.ui-dialog-titlebar {\n    display: block;\n    height: 18px;\n}\n.ui-dialog-title {\n    height: 10px;\n}</cssitem><cssitem location=\"inline\" name=\"addressAssociateCSS\" cachelocation=\"false\">.accountNameWrapper\n{\nfloat:left;\n}</cssitem></css></resources><styles><styleitem type=\"background\" bgtype=\"none\"/></styles></skuidpage>","skuid__Layout2__c":null,"skuid__Layout3__c":null,"skuid__Layout4__c":null,"skuid__Layout5__c":null}]