<aura:component controller="ARVaultService" implements="flexipage:availableForAllPageTypes,force:lightningQuickActionWithoutHeader" access="global" >
<aura:attribute name="objectData" type="Object[]"/>   
<aura:attribute name="uncheckedObjectData" type="Object[]"/>  
<aura:attribute name="excludedFields" type="Object"/>  
<aura:attribute name="objectsId" type="Integer"/>   
<aura:attribute name="currentObj" type="String"/>  
<aura:attribute name="fieldData" type="Object"/> 
<aura:attribute name="idSforg" type="String"/>      
<aura:handler name="sforgauthsucess" event="c:ARV_OrgSetup_SfAuth_Event" action="{!c.doInit}"/>
<div class="slds-page-header" aura:id="objectlist">
  <div class="slds-grid">
    <div class="slds-col slds-has-flexi-truncate">
      <div class="slds-media slds-no-space slds-grow">
        <div class="slds-media__figure">
          <span class="slds-icon_container" title=" ">
            <lightning:icon iconName="action:new_note" size="small" alternativeText="Indicates approval"/>
    	</span>
        </div>
        <div class="slds-media__body">
          <h1 class="slds-page-header__title slds-m-right_small slds-align-middle slds-truncate" title="Object List">Select Objects</h1>
        </div>
      </div>
    </div>
  </div>
<div class="componentHeight">
<table class="slds-table slds-table_cell-buffer">
        <thead>
        	<tr class="slds-text-title_caps header">
                <td class="slds-truncate">
            		<lightning:input type="checkbox" aura:id="Mcheckbox"  checked="true" label="API Name" name="Mcheckbox" onchange="{!c.checkAllCheckboxes}" />
                </td>
                <td class="slds-truncate">
            		Object Label Name
                </td>
                <td class="slds-truncate">
            		Excluded Fields
                </td>
            </tr>
        </thead>
        <tbody class="slds-scrollable_y">
        <aura:iteration items="{!v.objectData}" var="object" indexVar="index"> 
         <tr>
             <td>
            		<lightning:input type="checkbox" checked="true" label="{!object.name}" onchange="{!c.objectCheckboxChange}" name="{!index}" aura:id="oMcheckbox"/>
                </td>
                <td>
            		<span>{!object.label}</span>
                </td>
                <td>
                    <u>
                    <a href="javascript:void(0);" aura:id="parentExcludeCount">
                   		<span id="{!index}" aura:id="excludeCount" title="{!object.name}" name="{!object.name}" onclick="{!c.excludeFields}">
                        {!object.excludedFields.length>0?object.excludedFields.length:0}
                        </span> 
                    </a>
                        </u>
                </td>
            </tr>
           </aura:iteration>
		</tbody>
    </table>
</div>
</div>
<div class="slds-page-header slds-hide" aura:id="feildslist">
  <div class="slds-grid">
    <div class="slds-col slds-has-flexi-truncate">
      <div class="slds-media slds-no-space slds-grow">
        <div class="slds-media__figure">
          <span class="slds-icon_container" title=" ">
            <lightning:icon iconName="action:new_note" size="small" alternativeText="Indicates approval"/>
    	  </span>
        </div>
        <div class="slds-media__body">
          <p class="slds-line-height_reset">{!v.currentObj}</p>
          <h1 class="slds-page-header__title slds-m-right_small slds-align-middle slds-truncate" title="Object List">Select Fields</h1>
        </div>
      </div>
    </div>
    <div class="slds-col slds-no-flex slds-grid slds-align-top">
      <div class="slds-button-group" role="group">
        <button class="slds-button slds-button_neutral" aura:id="back" onclick="{!c.backToObjectsList}">Back</button>
      </div>
    </div>
  </div>
  <!--c:ARVOrgBackupShowFields gridData="{!v.fieldData}" excludedFields="{!v.excludedFields}"/-->
	<div class="componentHeight">
     <table class="slds-table slds-table_cell-buffer">
        <thead>
        	<tr class="slds-text-title_caps header">
                <td class="slds-truncate">
            		<lightning:input type="checkbox"  checked="true" aura:id="FCheckbox" label="Field API Name" name="FCheckbox" onchange="{!c.checkAllFieldCheckboxes}"/>
                </td>
                <td class="slds-truncate">
            		Label Name
                </td>
                <td class="slds-truncate">
            		Mask
                </td>
                <td class="slds-truncate">
            		Encrypt
                </td>
            </tr>
        </thead>
        <tbody class="slds-scrollable_y">
              <aura:iteration items="{!v.fieldData}" var="object" indexVar="index"> 
              <tr>
                <td>
            		<lightning:input type="checkbox" name="{!index}" label="{!object.name}" value="{!object.name}" aura:id="fMcheckbox" checked="true" onchange="{!c.fieldCheckboxChange}"/>
                </td>
                <td>
            		{!object.label}
                </td>
                <td>
                  <lightning:input type="toggle" name="{!index}" label=" " value="{!object.name}" aura:id="fieldmask" checked="false"/>
                </td>
               <td>
                  <lightning:input type="toggle" name="{!index}" label=" " value="{!object.name}" aura:id="fieldencrypted" checked="false"/>
                </td>
            </tr>
           </aura:iteration>
		</tbody>
    </table>
</div>
</div>
</aura:component>